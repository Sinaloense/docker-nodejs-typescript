{
    "name": "NodeJS Typescript",
    "dockerComposeFile": "docker-compose.yml",
    "service": "node",
    "workspaceFolder": "/app",
    "customizations": {
      "vscode": {
        "settings": {
          "editor.codeActionsOnSave": {
              "source.fixAll.eslint": "always"
          }
        },
        "extensions": [
          "dbaeumer.vscode-eslint"
        ]
      }
    }
}